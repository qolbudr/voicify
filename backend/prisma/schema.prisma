generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id      String    @unique
  name    String
  email   String    @unique
  photo   String 
  token   String    @db.Text 
  validAt DateTime? 
  createdAt DateTime @default(now())
}